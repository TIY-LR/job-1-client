<div class="col s10">
    {{#link-to "tracker.contacts.create"}}
        <i class="fa fa-plus"></i>
    {{/link-to}}
    {{#each model as |contact|}}
        {{#md-card title=contact.fullName id="contact-card"}}
            {{#md-card-content}}
                <ul>
                    <li>{{contact.officeNumber}}</li>
                    <li>{{contact.cellNumber}}</li>
                    <li>{{contact.email}}</li>
                    <li>{{contact.addressOne}}</li>
                    <li>{{contact.addressTwo}}</li>
                    <li>{{contact.city}}</li>
                    <li>{{contact.state}}</li>
                    <li>{{contact.zip}}</li>
                </ul>
            {{/md-card-content}}
            {{#md-card-action}}
                {{!-- {{#link-to "cards"}}Link 1{{/link-to}}
                {{#link-to "cards"}}Link 2{{/link-to}} --}}
            {{/md-card-action}}
        {{/md-card}}
    {{/each}}
</div>

{{outlet}}
